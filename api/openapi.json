{
  "openapi": "3.0.2",
  "info": {
    "title": "Qlueless Assembly Line API",
    "version": "0.0.1"
  },
  "paths": {
    "/dictionaries": {
      "get": {
        "description": "Returns all service dictionaires.",
        "responses": {
          "default": {
            "description": "Mapped list of dictionaries.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/dictionaries"
                }
              }
            }
          }
        }
      }
    },
    "/orders": {
    },
    "/batches": {
    }
  },
  "components": {
    "schemas": {
      "dictionaries": {
        "type": "object",
        "properties": {
          "tags": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/dictionary_tag"
            }
          },
          "statuses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/dictionary_status"
            }
          }
        }
      },
      "dictionary_tag": {
        "type": "object",
        "properties": {
          "title": {
            "$ref": "#/components/schemas/title"
          },
          "description": {
            "$ref": "#/components/schemas/description"
          },
          "tag_id": {
            "type": "string",
            "description": "Unique identifier of the tag."
          },
          "additional": {
            "$ref": "#/components/schemas/additional"
          }
        }
      },
      "dictionary_status": {
        "type": "object",
        "properties": {
          "title": {
            "$ref": "#/components/schemas/title"
          },
          "description": {
            "$ref": "#/components/schemas/description"
          },
          "status_id": {
            "type": "string",
            "description": "Unique identifier of the status."
          },
          "additional": {
            "$ref": "#/components/schemas/additional"
          }
        }
      },
      "title": {
        "type": "string",
        "description": "Presentable and human readable title of the entity instance."
      },
      "description": {
        "type": "string",
        "description": "Presentable and human readable description of the entity instance."
      },
      "additional": {
        "type": "string",
        "description": "Semicolon ';' separated list of additional attributes managed by clients. Each property is a key value pair using a colon ':' as the delimiter, i.e. 'key: value'."
      }
    }
  }
}