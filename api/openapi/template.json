{
	"openapi": "3.0.2",
	"info": {
		"title": "Go Qlueless Assembly API",
		"version": "0.0.1"
  },
  "tags": [
    {
      "name": "venture",
      "description": "All available Venture specific actions."
    },
    {
      "name": "order",
      "description": "All available Order specific actions."
    }
  ],
	"paths": {
    {{- "\n"}}{{ .Inject "/openapi/paths.json" 2}},
    {{- "\n"}}{{ .Inject "/changelog/paths.json" 2}},
    {{- "\n"}}{{ .Inject "/venture/paths.json" 2}},
    {{- "\n"}}{{ .Inject "/order/paths.json" 2}}
  },
	"components": {
    "parameters": {
      {{- "\n"}}{{ .Inject "/venture/parameters.json" 3}},
      {{- "\n"}}{{ .Inject "/order/parameters.json" 3}},
      {{- "\n"}}{{ .Inject "/shared/parameters.json" 3}}
    },
    "requestBodies": {
      {{- "\n"}}{{ .Inject "/venture/requestBodies.json" 3}},
      {{- "\n"}}{{ .Inject "/order/requestBodies.json" 3}}
    },
    "responses": {
      {{- "\n"}}{{ .Inject "/venture/responses.json" 3}},
      {{- "\n"}}{{ .Inject "/order/responses.json" 3}},
      {{- "\n"}}{{ .Inject "/shared/responses.json" 3}}
    },
		"schemas": {
      {{- "\n"}}{{ .Inject "/venture/schemas.json" 3}},
      {{- "\n"}}{{ .Inject "/order/schemas.json" 3}},
			{{- "\n"}}{{ .Inject "/shared/schemas.json" 3}}
    },
    "x-hidden": {
      {{- "\n"}}{{ .Inject "/venture/x-hidden.json" 3}},
      {{- "\n"}}{{ .Inject "/order/x-hidden.json" 3}},
      {{- "\n"}}{{ .Inject "/shared/x-hidden.json" 3}}
    }
	}
}
