"/batches": {
  "get": {
    "description": "Returns all batches.",
    "responses": {
      "default": {
        "description": "List of batches.",
        "content": {
          "application/json": {
            "schema": {
              "oneOf": [
                {
                  "$ref": "#/components/schemas/batches_wrapped"
                },
                {
                  "$ref": "#/components/schemas/batches_array"
                }
              ]
            }
          }
        }
      }
    }
  }
},
"/batches/{batch_id}": {
  "get": {
    "description": "Returns a single batch.",
    "parameters": [
      {
        "$ref": "#/components/parameters/batch_id"
      }
    ],
    "responses": {
      "default": {
        "description": "Single batch.",
        "content": {
          "application/json": {
            "schema": {
              "oneOf": [
                {
                  "$ref": "#/components/schemas/batch_wrapped"
                },
                {
                  "$ref": "#/components/schemas/batch_entry"
                }
              ]
            }
          }
        }
      }
    }
  }
}