    "description": "Returns all orders.",
    "parameters": [
      {
        "$ref": "#/components/parameters/wrap_with"
      }
    ],
    "responses": {
      "200": {
        "$ref": "#/components/responses/orders_200"
      },
      "default": {
        "$ref": "#/components/responses/error"
      }
    }
  },
  "post": {
    "description": "Creates a new order.",
