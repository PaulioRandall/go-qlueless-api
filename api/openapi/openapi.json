{
	"openapi": "3.0.2",
	"info": {
		"title": "Qlueless Assembly Line API",
		"version": "0.0.1"
	},
	"paths": {
		"/dictionaries": {
		  "get": {
		    "description": "Returns all service dictionaires.",
		    "responses": {
		      "default": {
		        "description": "Mapped list of dictionaries.",
		        "content": {
		          "application/json": {
		            "schema": {
		              "$ref": "#/components/schemas/dictionaries_reply"
		            }
		          }
		        }
		      }
		    }
		  }
		},
		"/orders": {
		  "get": {
		    "description": "Returns all orders.",
		    "responses": {
		      "default": {
		        "description": "List of orders.",
		        "content": {
		          "application/json": {
		            "schema": {
		              "$ref": "#/components/schemas/orders_reply"
		            }
		          }
		        }
		      }
		    }
		  }
		},
		"/batches": {
		  "get": {
		    "description": "Returns all batches.",
		    "responses": {
		      "default": {
		        "description": "List of batches.",
		        "content": {
		          "application/json": {
		            "schema": {
		              "$ref": "#/components/schemas/batches_reply"
		            }
		          }
		        }
		      }
		    }
		  }
		}
  },
	"components": {
		"schemas": {
			"dictionaries_reply": {
			  "type": "object",
			  "properties": {
			    "message": {
			      "$ref": "#/components/schemas/message"
			    },
			    "data": {
			      "type": "object",
			      "properties": {
			        "tags": {
			          "type": "array",
			          "items": {
			            "$ref": "#/components/schemas/tag_entry"
			          }
			        },
			        "statuses": {
			          "type": "array",
			          "items": {
			            "$ref": "#/components/schemas/status_entry"
			          }
			        },
			        "work_item_types": {
			          "type": "array",
			          "items": {
			            "$ref": "#/components/schemas/work_item_type_entry"
			          }
			        }
			      }
			    }
			  }
			},
			"tag_entry": {
			  "type": "object",
			  "properties": {
			    "title": {
			      "$ref": "#/components/schemas/title"
			    },
			    "description": {
			      "$ref": "#/components/schemas/description"
			    },
			    "tag_id": {
			      "$ref": "#/components/schemas/tag_id"
			    },
			    "additional": {
			      "$ref": "#/components/schemas/additional"
			    }
			  }
			},
			"status_entry": {
			  "type": "object",
			  "properties": {
			    "title": {
			      "$ref": "#/components/schemas/title"
			    },
			    "description": {
			      "$ref": "#/components/schemas/description"
			    },
			    "status_id": {
			      "$ref": "#/components/schemas/status_id"
			    },
			    "additional": {
			      "$ref": "#/components/schemas/additional"
			    }
			  }
			},
			"work_item_type_entry": {
			  "type": "object",
			  "properties": {
			    "title": {
			      "$ref": "#/components/schemas/title"
			    },
			    "description": {
			      "$ref": "#/components/schemas/description"
			    },
			    "work_item_type_id": {
			      "type": "string",
			      "description": "Unique identifier of the work item type."
			    },
			    "additional": {
			      "$ref": "#/components/schemas/additional"
			    }
			  }
			},
			"orders_reply": {
			  "type": "object",
			  "properties": {
			    "message": {
			      "$ref": "#/components/schemas/message"
			    },
			    "data": {
			      "type": "array",
			      "items": {
			        "$ref": "#/components/schemas/order_entry"
			      }
			    }
			  }
			},
			"order_entry": {
			  "type": "object",
			  "properties": {
			    "title": {
			      "$ref": "#/components/schemas/title"
			    },
			    "description": {
			      "$ref": "#/components/schemas/description"
			    },
			    "work_item_id": {
			      "$ref": "#/components/schemas/work_item_id"
			    },
			    "tag_id": {
			      "$ref": "#/components/schemas/tag_id"
			    },
			    "status_id": {
			      "$ref": "#/components/schemas/status_id"
			    },
			    "additional": {
			      "$ref": "#/components/schemas/additional"
			    }
			  }
			},
			"batches_reply": {
			  "type": "object",
			  "properties": {
			    "message": {
			      "$ref": "#/components/schemas/message"
			    },
			    "data": {
			      "type": "array",
			      "items": {
			        "$ref": "#/components/schemas/batch_entry"
			      }
			    }
			  }
			},
			"batch_entry": {
			  "type": "object",
			  "properties": {
			    "title": {
			      "$ref": "#/components/schemas/title"
			    },
			    "description": {
			      "$ref": "#/components/schemas/description"
			    },
			    "work_item_id": {
			      "$ref": "#/components/schemas/work_item_id"
			    },
			    "parent_work_item_id": {
			      "type": "string",
			      "description": "Unique identifier of the work items parent."
			    },
			    "tag_id": {
			      "$ref": "#/components/schemas/tag_id"
			    },
			    "status_id": {
			      "$ref": "#/components/schemas/status_id"
			    },
			    "additional": {
			      "$ref": "#/components/schemas/additional"
			    }
			  }
			},
			"message": {
			  "type": "string",
			  "description": "Human readable summary of the response in a form presentable to end users."
			},
			"title": {
			  "type": "string",
			  "description": "Presentable and human readable title of the entity instance."
			},
			"description": {
			  "type": "string",
			  "description": "Presentable and human readable description of the entity instance."
			},
			"work_item_id": {
			  "type": "string",
			  "description": "Unique identifier of the work item."
			},
			"tag_id": {
			  "type": "string",
			  "description": "Unique identifier of a tag."
			},
			"status_id": {
			  "type": "string",
			  "description": "Unique identifier of a status."
			},
			"additional": {
			  "type": "string",
			  "description": "Semicolon ';' separated list of additional attributes managed by clients. Each property is a key value pair using a colon ':' as the delimiter, i.e. 'key: value'."
			}
		}
	}
}
